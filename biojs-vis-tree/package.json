    "underscore": "^1.6.0"
  },
  "devDependencies": {
    "assert": "^1.1.1",
    "browserify": "~4.2.0",
    "chai": "^1.9.1",
    "coveralls": "~2.11.1",
    "debug": "^1.0.3",
    "istanbul": "~0.3.0",
    "mocha": "~1.20.1",
    "mocha-lcov-reporter": "0.0.1",
