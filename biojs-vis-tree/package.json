{
  "name": "biojs-vis-tree",
  "description": "Visualizing phylogenetic trees using d3",
  "version": "0.0.1",
  "homepage": "http://github.com/greenify/biojs",
  "repository": {
    "type": "git",
    "url": "git://github.com/greenify/biojs2"
  },
  "author": {
    "name": "BioJS",
    "email": "biojs-technical@googlegroups.com"
  },
  "dependencies": {
    "biojs-io-newick": "0.0.2",
