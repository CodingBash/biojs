{
    "name": "biojs-rest-ensembl",
    "description": "rest api for ensembl",
    "version": "0.0.1",
    "homepage": "http://biojs.net/",
    "repository": {
        "type": "git",
        "url": "git://github.com/biojs/biojs2"
    },
    "author": {
        "name": "David Dao",
        "email": "biojs-technical@googlegroups.com"
    },
    "dependencies": {
        "nets": "^1.0.0"
    },
    "devDependencies": {
        "assert": "^1.1.1",
        "browserify": "~4.2.0",
        "chai": "^1.9.1",
        "coveralls": "~2.11.1",
        "debug": "^1.0.3",
        "istanbul": "~0.3.0",
        "mocha": "~1.20.1",
        "mocha-lcov-reporter": "0.0.1",
        "should": "^4.0.4",
        "uglify-js": "~2.4.15",
        "underscore": "^1.6.0"
    },
    "keywords": [
    "biojs, rest, ensembl"
  ],
    "main": "index.js",
    "scripts": {
        "test": "mocha --reporter spec",
        "coverage": "istanbul cover ./node_modules/.bin/_mocha --report lcovonly -- -R spec && rm -rf ./coverage",
